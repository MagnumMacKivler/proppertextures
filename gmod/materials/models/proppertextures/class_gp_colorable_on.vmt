	"$EmissiveBlendTint"        "[1 1 1]"
	"$EmissiveBlendScrollVector" 	" [ 0 0 ] "
	
    "$envmap" "models\proppertextures\envmaps\specularity_75"
    "$envmaptint" "[0.106 0.106 0.102]"                 

    "Proxies"
    {
        "EnvMapDarken"
        {
            "min" "0"
            "max" "1"
            "envmap" "models\proppertextures\envmaps\specularity_75"
            "color" "[0.906 0.906 0.902]"
        }
		Equals
		{
			srcVar1 $color
			resultVar $emissiveBlendTint
		}
    }
}