"VertexLitGeneric"
{
	"$basetexture"			"models\proppertextures\paint\acrylic\acrylic_d"
	"$bumpmap" 			"models\proppertextures\paint\acrylic\acrylic_n"


	"$normalmapalphaenvmapmask"	"1"
	"$basemapalphaphongmask" 		"1"

	"$phong"			"1"
	"$phongalbedotint"		"0"
	"$phongtint"                  	"[1 1 1]"
vertexlitgeneric
{

	%tooltexture models/proppertextures/paint/acrylic/acrylic_d

	$basetexture models/proppertextures/paint/acrylic/acrylic_d
	$bumpmap models/proppertextures/paint/acrylic/acrylic_n
	$lightwarptexture dev/identitylightwarp

	$halflambert 1
	$ambientocclusion 1

	$normalmapalphaenvmapmask 1
	$basemapalphaphongmask 1

	$phong 1
	$phongalbedotint 0
	$phongtint "[ 1 1 1 ]"
	$phongboost 0.15
	$phongexponent 7
	$phongfresnelranges "[ 1 5 8 ]"

	$rimlight 1
	$rimlightexponent 10
	$rimlightboost 0.05

	$envmap env_cubemap
	$envmaptint "[ 0.015 0.015 0.015 ]"
	$envmapfresnel 0.5

	$detail detail/plaster_detail_01
	$detailscale 30.283
	$detailblendfactor 0.15
	$detailblendmode 0

	$color2 "{ 255 191 0 }"
	proxies
	{

		equals
		{

			srcvar1 $color2
			resultvar $color

		}
	
	}

}

	"$phongboost"                  	"0.15"
	"$phongexponent"             	 "7"
	"$phongfresnelranges"           "[1 5 8]"

	"$rimlight"               "1"
	"$rimlightexponent"       "10"
	"$rimlightboost"          "0.05"


	"$envmap"                       	"env_cubemap"
	"$envmaptint"                   	"[0.015 0.015 0.015]"
	"$envmapfresnel" 		"0.5"

	"$detail" 			"detail\plaster_detail_01"
	"$detailscale" 		"30.283"
	"$detailblendfactor" 	".15"
	"$detailblendmode" 	"0"

	"$override" "{255 191 0}"
	Proxies
	{
		Equals
		{
			srcVar1 $override
			resultVar $color
		}
	
	}
}
